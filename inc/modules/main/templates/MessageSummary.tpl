<!-- BEGIN section -->
<div class="section_alert">
	<div class="section_alert_title">
		{TITLE}
	</div>
	<table class="section_alert_entries">
	<!-- BEGIN entry -->
		<!-- BEGIN even -->
		<tr class="section_alert_entry table_row_even">
			<!-- BEGIN name -->
			<td class="section_alert_entry_name">{CONTENT}</td>
			<!-- END name -->
			
			<!-- BEGIN value -->
			
			<!-- BEGIN normal -->
			<td class="section_alert_entry_value_normal">{CONTENT}</td>
			<!-- END normal -->
			
			<!-- BEGIN emphasis -->
			<td class="section_alert_entry_value_emphasis">{CONTENT}</td>
			<!-- END emphasis -->
			
			<!-- END value -->
		</tr>
		<!-- END even -->
	
		<!-- BEGIN odd -->
		<tr class="section_alert_entry table_row_odd">
			<!-- BEGIN name -->
			<td class="section_alert_entry_name">{CONTENT}</td>
			<!-- END name -->
			
			<!-- BEGIN value -->
			
			<!-- BEGIN normal -->
			<td class="section_alert_entry_value_normal">{CONTENT}</td>
			<!-- END normal -->
			
			<!-- BEGIN emphasis -->
			<td class="section_alert_entry_value_emphasis">{CONTENT}</td>
			<!-- END emphasis -->
			
			<!-- END value -->
		</tr>
		<!-- END odd -->
	<!-- END entry -->
	</table>
</div>
<!-- END section -->
