[run]
branch = True
source =
    concerto_gui
omit =
    concerto_gui/siteconfig.py
    concerto_gui/web/wsgi.py

[report]
show_missing = True
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    pass
